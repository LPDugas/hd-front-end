{
  "name": "angular-akita-example",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration hmr",
    "start-dev": "ng serve",
    "test": "ng test",
    "test:ci": "yarn-or-npm run test --single-run --progress=false",
    "cypress:open": "cypress open",
    "cypress:ci": "cypress run",
    "build:prod": "yarn-or-npm run check:fix && ng build --prod --build-optimizer --vendor-chunk=true",
    "lint:check": "ng lint --type-check",
    "lint:fix": "yarn-or-npm run lint:check --fix",
    "check": "yarn-or-npm run lint:check && yarn-or-npm run prettier:check",
    "check:fix": "yarn-or-npm run lint:fix && yarn-or-npm run prettier:fix",
    "prettier:base-files": "yarn-or-npm run prettier \"./{cypress,src}/**/*.{scss,ts}\"",
    "prettier:fix": "yarn-or-npm run prettier:base-files --write",
    "prettier:check": "yarn-or-npm run prettier:base-files -l",
    "postbuild": "gulp compress"
  },
  "lint-staged": {
    "linters": {
      "*.{ts,scss}": [
        "yarn-or-npm run prettier -l"
      ]
    }
  },
  "private": true,
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "crypto": false,
    "stream": false,
    "http": false,
    "tls": false,
    "zlib": false,
    "https": false,
    "net": false
  },
  "dependencies": {
    "@angular/animations": "8.2.13",
    "@angular/cdk": "8.2.3",
    "@angular/common": "8.2.13",
    "@angular/compiler": "8.2.13",
    "@angular/core": "8.2.13",
    "@angular/flex-layout": "8.0.0-beta.27",
    "@angular/forms": "8.2.13",
    "@angular/material": "8.2.3",
    "@angular/platform-browser": "8.2.13",
    "@angular/platform-browser-dynamic": "8.2.13",
    "@angular/router": "8.2.13",
    "@angular/service-worker": "8.2.13",
    "@auth0/auth0-spa-js": "^1.3.2",
    "@datorama/akita": "1.24.2",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "ag-grid-angular": "21.2.2",
    "ag-grid-community": "21.2.2",
    "ajv": "^6.10.2",
    "angular-plotly.js": "1.3.2",
    "core-js": "2.6.3",
    "hammerjs": "2.0.8",
    "material-design-icons-iconfont": "4.0.4",
    "plotly.js": "^1.51.1",
    "plotly.js-dist": "^1.51.1",
    "rxjs": "6.5.3",
    "tslib": "~1.10.0",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.18",
    "@angular/cli": "8.3.18",
    "@angular/compiler-cli": "8.2.13",
    "@angular/language-service": "8.2.13",
    "@angularclass/hmr": "2.1.3",
    "@bahmutov/add-typescript-to-cypress": "2.0.0",
    "@datorama/akita-ngdevtools": "^3.0.2",
    "@types/jasmine": "3.3.7",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "10.12.18",
    "codelyzer": "4.5.0",
    "cypress": "3.1.4",
    "gulp": "4.0.0",
    "gulp-gzip": "1.4.2",
    "husky": "1.3.1",
    "jasmine-core": "3.3.0",
    "jasmine-spec-reporter": "4.2.1",
    "karma": "3.1.4",
    "karma-chrome-launcher": "2.2.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "karma-scss-preprocessor": "4.0.0",
    "lint-staged": "8.1.0",
    "listr": "^0.14.3",
    "prettier": "1.16.0",
    "ts-node": "8.0.1",
    "tslint": "5.12.1",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.5.3",
    "typescript-tslint-plugin": "^0.5.5",
    "wait-on": "3.2.0",
    "yarn-or-npm": "2.0.4"
  }
}
